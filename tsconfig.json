{
  "extends": "./node_modules/gts/tsconfig-google.json",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "*": [
        "types/*"
      ]
    },
    "rootDir": ".",
    "outDir": "build",
    "pretty": true,
    "module": "commonjs",
    "target": "es6",
    "strictNullChecks": true,
    "noUnusedLocals": true,
    "declaration": true
  },
  "include": [
    "src/**/*.ts",
    "test/**/*.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}